<div class="container challenges-pos text-center">
  <h3 class="text-center">Quiz </h3>
  <br><br>
  <div class="row text-center">
    <div class="col-md-4 card">
      <img class="card-img-top" src="../../img/dashboard/challenges/q1.jpeg" alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title">Level {{data.level}}</h5>
        {{#ifCond data.level}}
         <a href="/dashBoardRouter/quizQuestion" class="btn btn-outline-info">Take challenge</a>
        {{else}}
          <button class="btn btn-outline-info" disabled>Comming soon..</button>
        {{/ifCond}}
      </div>
    </div>
  </div>
</div>

<!--   Core JS Files   -->
<script src="../../script/external/jquery.min.js"></script>
<script src="../../script/external/bootstrap.min.js"></script>
<!-- Control Center for Now Ui Dashboard: for mobile -->
<script src="../../script/paper-dashboard.min.js" type="text/javascript"></script>

