<div class="container tab-pos">

  <ul class="nav nav-tabs" role="tablist">
    <li class="nav-item">
      <a class="nav-link active" data-toggle="tab" href="#1" role="tab">Programming Track</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" data-toggle="tab" href="#2" role="tab">Weekly challenge</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" data-toggle="tab" href="#3" role="tab">Problem Mark</a>
    </li>
  </ul>

  <div class="tab-content" role="tablist">
    <div class="tab-pane active" id="1" role="tabpanel">
      {{!--  --}}<br>
      <div class="col-md-6 card">
        <img class="card-img-top" src="../../img/dashboard/challenges/l1.jpg" alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title">Weekly Question</h5>
          <p class="card-text">Pattern Printing</p>
          <a href="/dashBoardRouter/code" class="btn btn-outline-primary">Take challenge</a>
        </div>
      </div>
      {{!--  --}}
    </div>
    <div class="tab-pane" id="2" role="tabpanel" style="font-family:monospace">
      {{!--  --}}<br>
      <div class="col-md-12 card">
        <div class="card-body">
          <div class="row">
            <div class="col-md-4">
              {{!--  --}}
              <div class="card">
                <img src="../../img/dashboard/1_tTQJFNpKFOOiH8dN_2tyEQ.png" alt="" class="img img-responsive">
                <div class="card-body">
                  <h5 class="card-title">Weekly Challenge</h5>
                  <p class="card-text">Your Code will be crossed Checked</p>
                  <label class="card-text">pts</label>
                  <h4>100</h4>
                </div>
              </div>
              {{!--  --}}
            </div>
            <div class="col-md-8">
              {{!--  --}}
              <div class="card">
                <div class="card-body">
                  <h3>Create a "Tic Tac Toe" Game in 'c' language</h3>
                </div>
              </div>
              <div class="card">
                <div class="card-body">
                  <h5>Interface should be like this..</h5>
                </div>
              </div>
              <div class="card">
                <div class="card-body">
                  <h5 class="card-title">Upload The Game in github</h5>
                  <form action="/codeRouter/game" method="POST">
                    <div class="form-group">
                      <label for="exampleLink">Place your github link here..</label>
                      <input type="link" name="link" class="form-control" placeholder="link">
                      <input type="hidden" value="{{programmer.username}}" name="username" />
                    </div>
                    <button type="submit" class="btn btn-outline-info">Post</button>
                  </form>
                </div>
              </div>
              {{!--  --}}
            </div>
          </div>
        </div>
      </div>
      {{!--  --}}
    </div>
    <div class="tab-pane" id="3" role="tabpanel">
      {{!--  --}}
      {{#each programmer.quiz.programmingTrack}}
      <div class="card card-user">
        <div class="card-header">
          <h5 class="card-title">Week 1</h5>
        </div>
        <div class="card-body">
          <div class="row">
            <div class="col-md-7">
              {{!-- table --}}
              <table class="table table-striped table-dark">
                <thead>
                  <tr>
                    <th scope="col">Problem No</th>
                    <th scope="col">Mark</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <th scope="row">1</th>
                    <td>{{problem1}}</td>
                  </tr>
                  <tr>
                    <th scope="row">2</th>
                    <td>{{problem2}}</td>
                  </tr>
                  <tr>
                    <th scope="row">3</th>
                    <td>{{problem3}}</td>
                  </tr>
                  <tr>
                    <th scope="row">4</th>
                    <td>{{problem4}}</td>
                  </tr>
                  <tr>
                    <th scope="row">5</th>
                    <td>{{problem5}}</td>
                  </tr>
                </tbody>
              </table>
              {{!-- table --}}
            </div>
            <div class="col-md-5 text-center">
              <label for="points" class="card-text">Points</label>
              <h1>{{points}}</h1><br>
              <label for="points" class="card-text">Status</label>
              <p>Attend</p><br>
              <label for="points" class="card-text">Time</label>
              <p>{{time}}</p><br>
            </div>
            {{!--  --}}
          </div>
        </div>
      </div>
      {{/each}}
    </div>
  </div>




  <!--   Core JS Files   -->

  <script src="../../script/external/jquery-slim.min.js"></script>
  <script src="../../script/external/tab.js"></script>
  <script src="../../script/external/util.js"></script>
  <script src="../../script/external/jquery.min.js"></script>
  <script src="../../script/external/bootstrap.min.js"></script>
  <!-- Control Center for Now Ui Dashboard: for mobile -->
  <script src="../../script/paper-dashboard.min.js" type="text/javascript"></script>